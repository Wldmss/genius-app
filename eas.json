{
    "cli": {
        "version": ">= 7.3.0"
    },
    "build": {
        "base": {
            "node": "20.11.1",
            "env": {
                "EXPO_PUBLIC_NAME": "지니어스",
                "EXPO_PUBLIC_WEB": "https://ktedu.kt.com",
                "EXPO_PUBLIC_TEST_SERVER_URL": "http://192.168.50.254:8080",
                "EXPO_PUBLIC_TEST_API_URL": "/api/v1/",
                "EXPO_PUBLIC_SERVER_URL": "https://ktedu.kt.com",
                "EXPO_PUBLIC_API_URL": "/"
            },
            "android": {
                "image": "latest",
                "buildType": "apk"
            },
            "ios": {
                "image": "latest"
            },
            "channel": "base"
        },
        "preview": {
            "extends": "base",
            "channel": "preview",
            "distribution": "internal"
        },
        "development": {
            "extends": "base",
            "channel": "development",
            "developmentClient": true,
            "distribution": "internal",
            "autoIncrement": true,
            "ios": {
                "credentialsSource": "local"
            },
            "env": {
                ".env": "./.env.development"
            }
        },
        "test": {
            "extends": "base",
            "channel": "test",
            "autoIncrement": true,
            "env": {
                ".env": "./.env.test"
            }
        },
        "staging": {
            "extends": "base",
            "channel": "staging",
            "autoIncrement": true,
            "env": {
                ".env": "./.env.staging"
            }
        },
        "production": {
            "extends": "base",
            "channel": "production",
            "autoIncrement": true,
            "env": {
                ".env": "./.env.production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
