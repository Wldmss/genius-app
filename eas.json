{
  "cli": {
    "version": ">= 7.3.0"
  },
  "build": {
    "base": {
      "node": "20.11.1",
      "env": {},
      "android": {
        "image": "latest",
        "buildType": "apk",
        "env": {
          "PLATFORM": "android"
        }
      },
      "ios": {
        "image": "latest",
        "env": {
          "PLATFORM": "ios"
        }
      },
      "channel": "base"
    },
    "development": {
      "extends": "base",
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "development",
        "SERVER_URL": "http://localhost:8081",
        "API_URL": "/api/v1"
      }
    },
    "preview": {
      "extends": "base",
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "APP_ENV": "preview",
        "SERVER_URL": "http://localhost:8081",
        "API_URL": "/api/v1"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "env": {
        "APP_ENV": "production",
        "SERVER_URL": "http://localhost:8081",
        "API_URL": "/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
