{
    "cli": {
        "version": ">= 7.3.0"
    },
    "build": {
        "base": {
            "node": "20.11.1",
            "env": {
                "EXPO_PUBLIC_NAME": "KT 그룹교육",
                "EXPO_PUBLIC_WEB": "https://ktedu.kt.com",
                "EXPO_PUBLIC_SERVER_URL": "https://ktedu.kt.com",
                "EXPO_PUBLIC_DEV_SERVER_URL": "https://dev.ktedu.kt.com:2443",
                "EXPO_PUBLIC_API_URL": "/",
                "EXPO_PUBLIC_TEST_API_URL": "/api/v1/"
            },
            "android": {
                "image": "latest",
                "buildType": "apk"
            },
            "ios": {
                "image": "latest"
            },
            "channel": "base"
        },
        "preview": {
            "extends": "base",
            "channel": "preview",
            "distribution": "internal"
        },
        "development": {
            "extends": "base",
            "channel": "development",
            "developmentClient": true,
            "distribution": "internal",
            "autoIncrement": true,
            "ios": {
                "credentialsSource": "local",
                "enterpriseProvisioning": "universal"
            },
            "env": {
                "EXPO_PUBLIC_PROFILE": "development"
            }
        },
        "test": {
            "extends": "base",
            "channel": "test",
            "autoIncrement": true,
            "ios": {
                "enterpriseProvisioning": "universal"
            },
            "env": {
                "EXPO_PUBLIC_PROFILE": "test"
            }
        },
        "staging": {
            "extends": "base",
            "channel": "staging",
            "autoIncrement": true,
            "env": {
                "EXPO_PUBLIC_PROFILE": "staging"
            }
        },
        "production": {
            "extends": "base",
            "channel": "production",
            "autoIncrement": true,
            "env": {
                "EXPO_PUBLIC_PROFILE": "production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
