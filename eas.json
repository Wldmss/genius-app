{
  "cli": {
    "version": ">= 7.3.0"
  },
  "build": {
    "base": {
      "node": "20.11.1",
      "env": {
        "EXPO_PUBLIC_VERSION": "1.0.0",
        "EXPO_PUBLIC_PROJECT_ID": "bd698549-609f-476e-9339-4d0566f95b31",
        "EXPO_PUBLIC_WEB": "http://192.168.219.254:8080"
      },
      "android": {
        "image": "latest",
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_PLATFORM": "android"
        }
      },
      "ios": {
        "image": "latest",
        "env": {
          "EXPO_PUBLIC_PLATFORM": "ios"
        }
      },
      "channel": "base"
    },
    "development": {
      "extends": "base",
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_PROFILE": "development",
        "EXPO_PUBLIC_SERVER_URL": "http://localhost:8081",
        "EXPO_PUBLIC_API_URL": "/api/v1"
      }
    },
    "preview": {
      "extends": "base",
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_PROFILE": "preview",
        "EXPO_PUBLIC_SERVER_URL": "http://localhost:8081",
        "EXPO_PUBLIC_API_URL": "/api/v1"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_PROFILE": "production",
        "EXPO_PUBLIC_SERVER_URL": "http://localhost:8081",
        "EXPO_PUBLIC_API_URL": "/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
