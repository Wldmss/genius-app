{
    "name": "genius-app",
    "version": "1.0.0",
    "main": "expo-router/entry",
    "scripts": {
        "start": "set EXPO_PUBLIC_PROFILE=preview && npx expo start --clear",
        "start:tunnel": "set EXPO_PUBLIC_PROFILE=preview && npx expo start --tunnel --clear",
        "dev": "set EXPO_PUBLIC_PROFILE=development && expo start --dev-client --reset-cache",
        "dev:tunnel": "set EXPO_PUBLIC_PROFILE=development && expo start --dev-client --tunnel --reset-cache",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "configure": "npx eas build:configure",
        "prebuild": "npx expo prebuild --clean",
        "build:preview": "npx eas build -p android --profile preview",
        "build:dev": "npx eas build -p android --profile development",
        "build:prod": "npx eas build -p android --profile production",
        "update:dev": "npx eas update --branch development --message",
        "update": "npx eas update:configure"
    },
    "dependencies": {
        "@expo/metro-runtime": "~3.1.3",
        "@react-native-firebase/app": "^19.0.1",
        "@react-native-firebase/messaging": "^19.0.1",
        "axios": "^1.6.7",
        "expo": "~50.0.13",
        "expo-barcode-scanner": "~12.9.3",
        "expo-build-properties": "~0.11.1",
        "expo-camera": "~14.1.1",
        "expo-clipboard": "~5.0.1",
        "expo-constants": "~15.4.5",
        "expo-device": "~5.9.3",
        "expo-file-system": "~16.0.8",
        "expo-image-picker": "~14.7.1",
        "expo-linking": "~6.2.2",
        "expo-local-authentication": "~13.8.0",
        "expo-notifications": "~0.27.6",
        "expo-router": "~3.4.8",
        "expo-screen-orientation": "~6.4.1",
        "expo-secure-store": "~12.8.1",
        "expo-status-bar": "~1.11.1",
        "expo-system-ui": "~2.9.3",
        "expo-updates": "~0.24.12",
        "expo-web-browser": "~12.8.2",
        "firebase": "^10.8.1",
        "moment": "^2.30.1",
        "prop-types": "^15.8.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-native": "0.73.5",
        "react-native-crypto-js": "^1.0.0",
        "react-native-dotenv": "^3.4.10",
        "react-native-gesture-handler": "~2.14.0",
        "react-native-safe-area-context": "4.8.2",
        "react-native-screens": "~3.29.0",
        "react-native-svg": "14.1.0",
        "react-native-svg-transformer": "^1.3.0",
        "react-native-web": "~0.19.6",
        "react-native-webview": "13.6.4",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.22.2",
        "redux": "^5.0.1",
        "redux-thunk": "^3.1.0",
        "expo-task-manager": "~11.7.2",
        "expo-splash-screen": "~0.26.4",
        "expo-font": "~11.10.3"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "babel-plugin-module-resolver": "^5.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5"
    },
    "private": true
}
