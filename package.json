{
    "name": "genius-app",
    "version": "1.0.0",
    "main": "expo-router/entry",
    "scripts": {
        "start": "set EXPO_PUBLIC_PROFILE=preview && npx expo start --clear",
        "start:tunnel": "set EXPO_PUBLIC_PROFILE=preview && npx expo start --tunnel --clear",
        "dev": "expo start --dev-client --reset-cache --clear",
        "dev:tunnel": "expo start --dev-client --tunnel --reset-cache --clear",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "configure": "npx eas build:configure",
        "prebuild": "npx expo prebuild --clean",
        "build:preview": "npx eas build -p android --profile preview --clear-cache",
        "build:dev": "npx eas build -p android --profile development --clear-cache",
        "build:test": "npx eas build -p android --profile test --clear-cache",
        "build:sta": "npx eas build -p android --profile staging --clear-cache",
        "build:prod": "npx eas build -p android --profile production --clear-cache",
        "build:ios:dev": "npx eas build -p ios --profile development --clear-cache",
        "build:ios:test": "npx eas build -p ios --profile test --clear-cache",
        "build:ios:sta": "npx eas build -p ios --profile staging --clear-cache",
        "build:ios:prod": "npx eas build -p ios --profile production --clear-cache",
        "credentials": "npx eas credentials",
        "update:test": "set EXPO_PUBLIC_PROFILE=test && npx eas update --branch test --message ",
        "update:sta": "set EXPO_PUBLIC_PROFILE=staging && npx eas update --branch staging --message ",
        "update:prod": "set EXPO_PUBLIC_PROFILE=production && npx eas update --branch production --message ",
        "update": "npx eas update:configure",
        "clear": "npx expo build:clear",
        "doctor": "npx -y expo-doctor"
    },
    "dependencies": {
        "@expo/metro-runtime": "~3.2.1",
        "@react-native-async-storage/async-storage": "1.23.1",
        "@react-native-firebase/app": "^20.0.0",
        "@react-native-firebase/messaging": "^20.0.0",
        "axios": "^1.6.7",
        "expo": "^51.0.11",
        "expo-barcode-scanner": "~13.0.1",
        "expo-build-properties": "~0.12.3",
        "expo-camera": "~15.0.10",
        "expo-clipboard": "~6.0.3",
        "expo-constants": "~16.0.2",
        "expo-dev-client": "~4.0.15",
        "expo-device": "~6.0.2",
        "expo-file-system": "~17.0.1",
        "expo-font": "~12.0.7",
        "expo-image-picker": "~15.0.5",
        "expo-keep-awake": "~13.0.2",
        "expo-linking": "~6.3.1",
        "expo-local-authentication": "~14.0.1",
        "expo-notifications": "~0.28.7",
        "expo-router": "~3.5.15",
        "expo-screen-orientation": "~7.0.5",
        "expo-secure-store": "~13.0.1",
        "expo-splash-screen": "~0.27.5",
        "expo-status-bar": "~1.12.1",
        "expo-system-ui": "~3.0.5",
        "expo-task-manager": "~11.8.2",
        "expo-updates": "~0.25.16",
        "expo-video": "^1.1.10",
        "expo-web-browser": "~13.0.3",
        "firebase": "^10.8.1",
        "lottie-react-native": "6.7.0",
        "moment": "^2.30.1",
        "prop-types": "^15.8.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-native": "0.74.2",
        "react-native-crypto-js": "^1.0.0",
        "react-native-dotenv": "^3.4.10",
        "react-native-file-viewer": "^2.1.5",
        "react-native-fs": "^2.20.0",
        "react-native-gesture-handler": "~2.16.1",
        "react-native-pager-view": "6.3.0",
        "react-native-paper": "^5.12.3",
        "react-native-safe-area-context": "4.10.1",
        "react-native-screens": "3.31.1",
        "react-native-svg": "15.2.0",
        "react-native-svg-transformer": "^1.3.0",
        "react-native-web": "~0.19.6",
        "react-native-webview": "13.8.6",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.22.2",
        "redux": "^5.0.1",
        "redux-thunk": "^3.1.0",
        "rn-fetch-blob": "^0.12.0"
    },
    "devDependencies": {
        "@babel/core": "^7.24.0",
        "babel-plugin-module-resolver": "^5.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5"
    },
    "private": true
}
